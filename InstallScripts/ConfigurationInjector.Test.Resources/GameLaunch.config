      <endpoint name="Player" contract="BedeGaming.Wallet.v1.IPlayerContract" address="http://dev61-sgp.bedegaming.com:18261/Player" behaviorConfiguration="webJsonOverHttpBehavior" binding="webHttpBinding" bindingConfiguration="webHttpBindingConfig" />
    </client>
  </system.serviceModel>
