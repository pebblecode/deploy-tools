<?xml version="1.0" ?>
<envConfiguration>
  
  <environment.name>Production</environment.name>
  <environment.httpHostName>localhost</environment.httpHostName>
  <environment.GameRT.EndpointAddress>net.pipe://localhost/Gaming/1/3</environment.GameRT.EndpointAddress>
  
  <bingo.dbHost>localhost</bingo.dbHost>
  <bingo.dbConnectionstring>Database={0};Data Source={1};Port={2};User Id=freeBingo;Password=freeBingo;</bingo.dbConnectionstring>
  <bingo.dbName>fb_dev</bingo.dbName>
  <bingo.dbPortNumber>3400</bingo.dbPortNumber>
  <bingo.firstHttpPortNumber>2201</bingo.firstHttpPortNumber>
  <bingo.lastHttpPortNumber>2260</bingo.lastHttpPortNumber>
  <bingo.profileLogPeriod>600000</bingo.profileLogPeriod>
  <bingo.secondsBeforeClearingProfileStats>600</bingo.secondsBeforeClearingProfileStats>
  <bingo.randomNumberGeneratorServiceUrl>http://31.222.164.54/RandomNumber.svc/{host}/{game}/generate$format=json</bingo.randomNumberGeneratorServiceUrl>
  <bingo.deadZoneStartTimeHours>4</bingo.deadZoneStartTimeHours>
  <bingo.deadZoneStartTimeMinutes>50</bingo.deadZoneStartTimeMinutes>
  <bingo.deadZonePeriodMinutes>16</bingo.deadZonePeriodMinutes>
  <bingo.minSecondsBetweenClientHeartbeats>10</bingo.minSecondsBetweenClientHeartbeats>
  <bingo.heartbeatGroupsPerRoom>10</bingo.heartbeatGroupsPerRoom>
  <bingo.heartbeatOffsetPerRoomSeconds>10</bingo.heartbeatOffsetPerRoomSeconds>
  
  <bingo.systemLogListener.fileName>..\logs\_engine.log</bingo.systemLogListener.fileName>
  <bingo.serverLogListener.fileName>..\logs\_engine_errors.log</bingo.serverLogListener.fileName>
  <bingo.systemProfilerLogListener.fileName>..\logs\_engine_status.log</bingo.systemProfilerLogListener.fileName>
  <bingo.dbProfilerLogListener.fileName>..\logs\_engine_db_profile.log</bingo.dbProfilerLogListener.fileName>
  <bingo.messageProfilerLogListener.fileName>..\logs\_engine_msg_profile.log</bingo.messageProfilerLogListener.fileName>
  <bingo.badIpLogListener.fileName>..\logs\_engine_junk_ips.log</bingo.badIpLogListener.fileName>
  <bingo.clientErrorLogListener.fileName>..\logs\_client_errors.log</bingo.clientErrorLogListener.fileName>

  <bingo.gameLog.categorySource>Information</bingo.gameLog.categorySource>
  <bingo.messageBadIpLog.categorySource>All</bingo.messageBadIpLog.categorySource>
  <bingo.messageBrokerLog.categorySource>Information</bingo.messageBrokerLog.categorySource>
  <bingo.roomLog.categorySource>Information</bingo.roomLog.categorySource>
  <bingo.messageDataLog.categorySource>Off</bingo.messageDataLog.categorySource>
  <bingo.schedulerLog.categorySource>Information</bingo.schedulerLog.categorySource>
  <bingo.serverErrorLog.categorySource>All</bingo.serverErrorLog.categorySource>
  <bingo.generalErrorLog.categorySource>All</bingo.generalErrorLog.categorySource>
  <bingo.dbProfilerLog.categorySource>Information</bingo.dbProfilerLog.categorySource>
  <bingo.msgProfilerLog.categorySource>Information</bingo.msgProfilerLog.categorySource>
  <bingo.sysProfilerLog.categorySource>All</bingo.sysProfilerLog.categorySource>
  <bingo.lockingLog.categorySource>Off</bingo.lockingLog.categorySource>
  <bingo.taskLog.categorySource>All</bingo.taskLog.categorySource>
  <bingo.robotLog.categorySource>Information</bingo.robotLog.categorySource>
  <bingo.jsonpClientProxyLog.categorySource>Information</bingo.jsonpClientProxyLog.categorySource>
  <bingo.httpMessagingLog.categorySource>Information</bingo.httpMessagingLog.categorySource>
  <bingo.sgpMediatorLog.categorySource>Information</bingo.sgpMediatorLog.categorySource>

  <bingo.allEventsLog.specialSource>All</bingo.allEventsLog.specialSource>
  <bingo.notProcessedLog.specialSource>All</bingo.notProcessedLog.specialSource>
  <bingo.errorsLog.specialSource>All</bingo.errorsLog.specialSource>

  <bingo.webXmlOverHttpEndpointBehavior.helpEnabled>true</bingo.webXmlOverHttpEndpointBehavior.helpEnabled>
  <bingo.webJsonOverHttpEndpointBehaviour.helpEnabled>true</bingo.webJsonOverHttpEndpointBehaviour.helpEnabled>
  <bingo.netNamedPipeEndpointBehaviour.integrationToken>Bingo</bingo.netNamedPipeEndpointBehaviour.integrationToken>


  <bingo.myServiceTypeServiceBehavior.httpGetEnabled>true</bingo.myServiceTypeServiceBehavior.httpGetEnabled>
  <bingo.myServiceTypeServiceBehavior.includeExceptionDetailInFaults>true</bingo.myServiceTypeServiceBehavior.includeExceptionDetailInFaults>
  <bingo.returnFaultsServiceBehavior.includeExceptionDetailInFaults>true</bingo.returnFaultsServiceBehavior.includeExceptionDetailInFaults>

  <bingo.webHttpBindingConfig.hostNameComparisonMode>Exact</bingo.webHttpBindingConfig.hostNameComparisonMode>
  <bingo.webHttpsBindingConfig.hostNameComparisonMode>Exact</bingo.webHttpsBindingConfig.hostNameComparisonMode>

  <core.dbHost>db.bedegaming.com</core.dbHost>
  <core.dbUser>gambler</core.dbUser>

</envConfiguration>